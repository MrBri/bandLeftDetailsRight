<div class="col-xs-5 col-sm-4 nopadding">
  <ul class="nav nav-pills nav-stacked nopadding">
    <% bandNames.forEach(function(name, i) {  %>
      <li data-band-id="<%= i + 1 %>" class="bandName <% if (bandId == (i + 1)) { %>active<% } %>">
        <a href="/bands/<%= i + 1 %>"><%= name %></a>
      </li>
    <% }) %>
  </ul>
</div>

<div class="col-xs-7 col-sm-8">
  <div id="<%= bandId %>">
    <dl class="dl-horizontal">
      <dt>Year formed</dt>
      <dd><%= bandYear %></dd>
      <dt>Members</dt>
      <dd>
        <ul class="list-unstyled">
        <% bandMembers.forEach(function(member) { %>
          <li><%= member.name %></li>
        <% }) %>
        </ul>
      </dd>
    </dl>
  </div>
</div>
